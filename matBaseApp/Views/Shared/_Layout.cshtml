<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/css")
    @Scripts.Render("~/js")
    @RenderSection("scripts", required: false)
</head>
<body>
    <div id="header">
        @Html.Partial("_Header")
    </div>
    <div id="body">
        @RenderBody()
    </div>
    @*<div id="footer">
        @Html.Partial("_Footer")
    </div>*@
</body>
</html>
